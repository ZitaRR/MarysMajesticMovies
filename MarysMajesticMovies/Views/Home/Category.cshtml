@model IEnumerable<Movie>
@{
    ViewData["Title"] = "Category";
}

    <div class="content">
        <div class="banner4">
        </div>
        <div class="category">
            <div>
                @foreach (var item in Model)
                {
                    <a asp-action="Detail" asp-route-id="@item.Id"><img src="@item.PosterUrl" class="movieCategory"></a>
                }
            </div>
        </div>
    </div>

